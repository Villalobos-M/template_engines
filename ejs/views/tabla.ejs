<div>

   <% if(arrayProductos.length >= 1){ %>
      <table>
         <tr>
            <th>Nombre</th>
            <th>Precio</th>
            <th>Imagen</th>
         </tr>
         <% arrayProductos.forEach(producto=>{ %>
            <tr>
               <td>
                  <%= producto.nombre %>
               </td>
               <td>
                  <%= producto.precio %>
               </td>
               <td>
                  <img src=<%= producto.url %> alt=<%=producto.nombre %> >
                  
               </td>
            </tr>
            <% }) %>
      </table> 
   <% } else { %>
      <h2>No hay productos</h2>
      <% } %>


</div>